        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>System.TimeSpan</source>
          <target state="translated">System.TimeSpan</target>       </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Measure-Command<ept id="p1">**</ept> returns a time span object that represents the result.</source>
          <target state="translated"><bpt id="p1">**</bpt>Comando Measure<ept id="p1">**</ept> devuelve un objeto de intervalo de tiempo que representa el resultado.</target>       </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>NOTES</source>
          <target state="translated">NOTAS</target>       </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>For more information, type <ph id="ph1">`Get-Help Measure-Command -detailed`</ph>.</source>
          <target state="translated">Para obtener más información, escriba <ph id="ph1">`Get-Help Measure-Command -detailed`</ph>.</target>       </trans-unit>
        <trans-unit id="135" translate="yes" xml:space="preserve">
          <source>For technical information, type <ph id="ph1">`Get-Help Measure-Command -full`</ph>.</source>
          <target state="translated">Para obtener información técnica, escriba <ph id="ph1">`Get-Help Measure-Command -full`</ph>.</target>       </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>When specifying multiple values for a parameter, use commas to separate the values.</source>
          <target state="translated">Al especificar varios valores para un parámetro, use comas para separar los valores.</target>       </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>For example, <ph id="ph1">\&lt;</ph>parameter-name<ph id="ph2">\&gt;</ph> <ph id="ph3">\&lt;</ph>value1<ph id="ph4">\&gt;</ph>, <ph id="ph5">\&lt;</ph>value2<ph id="ph6">\&gt;</ph>.</source>
          <target state="translated">Por ejemplo, <ph id="ph1">\&lt;</ph>nombre del parámetro<ph id="ph2">\&gt;</ph> <ph id="ph3">\&lt;</ph>value1<ph id="ph4">\&gt;</ph>, <ph id="ph5">\&lt;</ph>value2<ph id="ph6">\&gt;</ph>.</target>       </trans-unit>
