          <source>Example 1: Get all package sources</source>
          <target state="translated">Ejemplo 1: Obtener todos los orígenes de paquetes</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This command gets all package sources that are registered with Package Management on the local computer.</source>
          <target state="translated">Este comando obtiene todos los orígenes de paquetes que están registrados con el paquete de administración en el equipo local.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Example 2: Get all package sources for a specific provider</source>
          <target state="translated">Ejemplo 2: Obtener todos los orígenes de paquetes para un proveedor específico</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>This command gets all package sources that are registered for the PSModule provider.</source>
          <target state="translated">Este comando obtiene todos los orígenes de paquetes que están registrados para el proveedor de PSModule.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Example 3: Get all package sources for a specific provider</source>
