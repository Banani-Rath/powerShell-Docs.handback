        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>Gets only the specified trace sources.</source>
          <target state="translated">Obtient uniquement les sources de suivi spécifiées.</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>Wildcards are permitted.</source>
          <target state="translated">Les caractères génériques sont autorisés.</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>The parameter name ("Name") is optional.</source>
          <target state="translated">Le nom de paramètre (« Name ») est facultatif.</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
