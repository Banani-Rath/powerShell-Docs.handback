        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>PS C:\\&amp;gt; $tempFile = New-TemporaryFile</source>
          <target state="translated">PS C:\\&amp;gt; $tempFile = New-TemporaryFile</target>         
        </trans-unit>
