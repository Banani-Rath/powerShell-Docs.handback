          <source>Split-Path</source>
          <target state="translated">Split-Path</target>       </trans-unit>
        <trans-unit id="154" translate="yes" xml:space="preserve">
          <source>Test-Path</source>
          <target state="translated">Test-Path</target>       </trans-unit>
        <trans-unit id="155" translate="yes" xml:space="preserve">
          <source>Get-PSProvider</source>
          <target state="translated">Get-psprovider</target>       </trans-unit>
