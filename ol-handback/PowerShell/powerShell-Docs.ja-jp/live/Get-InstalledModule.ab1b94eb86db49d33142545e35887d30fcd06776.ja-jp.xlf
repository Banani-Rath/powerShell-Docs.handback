        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Gets installed modules on a computer.</source>
          <target state="translated">コンピューター上のインストールされているモジュールを取得します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
