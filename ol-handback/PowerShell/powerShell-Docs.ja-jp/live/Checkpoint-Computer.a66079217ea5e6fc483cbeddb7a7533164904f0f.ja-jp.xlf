        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Checkpoint-Computer</source>
          <target state="translated">Checkpoint-Computer</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Checkpoint-Computer</source>
          <target state="translated">Checkpoint-Computer</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">概要</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Creates a system restore point on the local computer.</source>
          <target state="translated">ローカル コンピューターのシステム復元ポイントを作成します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">構文</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">説明</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cmdlet creates a system restore point on the local computer.</source>
          <target state="translated"> <bpt id="p1">**</bpt>Checkpoint-computer<ept id="p1">**</ept> コマンドレットは、ローカル コンピューターのシステム復元ポイントを作成します。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>System restore points and the <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cmdlet are supported only on client operating systems, such as Windows 8, Windows 7, Windows Vista, and Windows XP.</source>
          <target state="translated">システムの復元ポイントおよび <bpt id="p1">**</bpt>Checkpoint-computer<ept id="p1">**</ept> コマンドレットは、Windows 8、Windows 7、Windows Vista および Windows XP などのクライアント オペレーティング システムでのみサポートされます。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Beginning in Windows 8, <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cannot create more than one checkpoint each day.</source>
          <target state="translated">Windows 8 で始まる <bpt id="p1">**</bpt>Checkpoint-computer<ept id="p1">**</ept> 1 日に 1 つ以上のチェックポイントを作成できません。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">例</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Example 1: Create a system restore point</source>
          <target state="translated">例 1: システムの復元ポイントを作成します。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This command creates a system restore point called Install MyApp.</source>
          <target state="translated">このコマンドでは、MyApp のインストールと呼ばれるシステムの復元ポイントを作成します。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>It uses the default APPLICATION_INSTALL restore point type.</source>
          <target state="translated">復元ポイントの種類には、既定値の APPLICATION_INSTALL を使用します。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Example 2: Create a system MODIFY_SETTINGS restore point</source>
          <target state="translated">例 2: MODIFY_SETTINGS システムの復元ポイントを作成します。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>This command creates a MODIFY_SETTINGS system restore point called "ChangeNetSettings".</source>
          <target state="translated">このコマンドは、"ChangeNetSettings" という名前の MODIFY_SETTINGS システム復元ポイントを作成します。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">パラメータ</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>-Description</source>
          <target state="translated">-Description</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Specifies a descriptive name for the restore point.</source>
          <target state="translated">復元ポイントのわかりやすい名前を指定します。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>This parameter is required.</source>
          <target state="translated">このパラメーターは必須です。</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>-RestorePointType</source>
          <target state="translated">-RestorePointType</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Specifies the type of restore point.</source>
          <target state="translated">復元ポイントの種類を指定します。</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>The default is APPLICATION_INSTALL.</source>
          <target state="translated">既定値は APPLICATION_INSTALL です。</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>The acceptable values for this parameter are:</source>
          <target state="translated">このパラメーターに対して使用できる値:</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>APPLICATION_INSTALL</source>
          <target state="translated">APPLICATION_INSTALL</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>APPLICATION_UNINSTALL</source>
          <target state="translated">APPLICATION_UNINSTALL</target>       </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>DEVICE_DRIVER_INSTALL</source>
          <target state="translated">DEVICE_DRIVER_INSTALL</target>       </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>MODIFY_SETTINGS</source>
          <target state="translated">MODIFY_SETTINGS</target>       </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>CANCELLED_OPERATION</source>
          <target state="translated">CANCELLED_OPERATION です。</target>       </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>CommonParameters</source>
          <target state="translated">CommonParameters</target>       </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>This cmdlet supports the common parameters: -Debug, -ErrorAction, -ErrorVariable, -InformationAction, -InformationVariable, -OutVariable, -OutBuffer, -PipelineVariable, -Verbose, -WarningAction, and -WarningVariable.</source>
          <target state="translated">このコマンドレットは、共通のパラメーターをサポートしています:-デバッグ、-erroraction、-errorvariable、- InformationAction-InformationVariable、- OutVariable、-OutBuffer、- PipelineVariable、- Verbose、および - WarningVariable です。</target>       </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>For more information, see about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216).</source>
          <target state="translated">詳細については、about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216) を参照してください。</target>       </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>INPUTS</source>
          <target state="translated">入力</target>       </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>None</source>
          <target state="translated">None</target>       </trans-unit>
        <trans-unit id="135" translate="yes" xml:space="preserve">
          <source>You cannot pipe objects to <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept>.</source>
          <target state="translated">オブジェクトをパイプすることはできません <bpt id="p1">**</bpt>Checkpoint-computer<ept id="p1">**</ept>します。</target>       </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>OUTPUTS</source>
          <target state="translated">出力</target>       </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>None</source>
          <target state="translated">None</target>       </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>This cmdlet does not generate any output.</source>
          <target state="translated">このコマンドレットは出力を生成しません。</target>       </trans-unit>
        <trans-unit id="139" translate="yes" xml:space="preserve">
          <source>NOTES</source>
          <target state="translated">ノート</target>       </trans-unit>
        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>This cmdlet uses the <bpt id="p1">**</bpt>CreateRestorePoint<ept id="p1">**</ept> method of the <bpt id="p2">**</bpt>SystemRestore<ept id="p2">**</ept> class with a <bpt id="p3">**</bpt>BEGIN_SYSTEM_CHANGE<ept id="p3">**</ept> event.</source>
          <target state="translated">このコマンドレットを使用して、 <bpt id="p1">**</bpt>CreateRestorePoint<ept id="p1">**</ept> のメソッド、 <bpt id="p2">**</bpt>SystemRestore<ept id="p2">**</ept> クラス、 <bpt id="p3">**</bpt>BEGIN_SYSTEM_CHANGE<ept id="p3">**</ept> イベントです。</target>       </trans-unit>
        <trans-unit id="141" translate="yes" xml:space="preserve">
          <source>Beginning in Windows 8, <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cannot create more than one system restore point each day.</source>
          <target state="translated">Windows 8 で始まる <bpt id="p1">**</bpt>Checkpoint-computer<ept id="p1">**</ept> 1 日に 1 つ以上のシステム復元ポイントを作成できません。</target>       </trans-unit>
        <trans-unit id="142" translate="yes" xml:space="preserve">
          <source>If you try to create a new restore point before the 24-hour period has elapsed, Windows PowerShell generates the following error:</source>
          <target state="translated">24 時間が経過する前に新しい復元ポイントを作成しようとすると、Windows PowerShell によって次のエラーが生成されます。</target>       </trans-unit>
