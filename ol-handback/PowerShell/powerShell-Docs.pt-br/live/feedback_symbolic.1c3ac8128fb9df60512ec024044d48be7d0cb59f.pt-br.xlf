        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>SYMBOLIC LINK DIRECTORIES</source>
          <target state="translated">DIRETÓRIOS DE LINKS SIMBÓLICOS</target>         
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>HARD LINKS</source>
          <target state="translated">LINKS FÍSICOS</target>         
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>DIRECTORY JUNCTIONS</source>
          <target state="translated">JUNÇÕES DE DIRETÓRIO</target>         
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>GET-CHILDITEM</source>
