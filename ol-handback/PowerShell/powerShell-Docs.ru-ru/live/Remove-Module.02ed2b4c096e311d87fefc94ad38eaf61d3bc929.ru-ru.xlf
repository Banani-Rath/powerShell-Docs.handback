          <source>INPUTS</source>
          <target state="translated">ВХОДНЫЕ ДАННЫЕ</target>       </trans-unit>
        <trans-unit id="151" translate="yes" xml:space="preserve">
          <source>System.String, System.Management.Automation.PSModuleInfo</source>
          <target state="translated">System.String, System.Management.Automation.PSModuleInfo</target>       </trans-unit>
        <trans-unit id="152" translate="yes" xml:space="preserve">
          <source>You can pipe module names (strings) and module objects to <bpt id="p1">**</bpt>Remove-Module<ept id="p1">**</ept>.</source>
          <target state="translated">Можно передать имена (строки) и объекты модуля <bpt id="p1">**</bpt>Remove-Module<ept id="p1">**</ept>.</target>       </trans-unit>
        <trans-unit id="153" translate="yes" xml:space="preserve">
          <source>OUTPUTS</source>
          <target state="translated">ВЫХОДНЫЕ ДАННЫЕ</target>       </trans-unit>
        <trans-unit id="154" translate="yes" xml:space="preserve">
          <source>None</source>
          <target state="translated">Нет</target>       </trans-unit>
