      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>DSC Registry Resource</source>
          <target state="translated">Risorsa Registry DSC</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>powershell,DSC</source>
          <target state="translated">powershell,DSC</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>DSC Registry Resource</source>
          <target state="translated">Risorsa Registry DSC</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Applies To: Windows PowerShell 4.0, Windows PowerShell 5.0</source>
          <target state="translated">Si applica a: Windows PowerShell 4.0, Windows PowerShell 5.0</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Registry<ept id="p1">**</ept> resource in Windows PowerShell Desired State Configuration (DSC) provides a mechanism to manage registry keys and values on a target node.</source>
          <target state="translated">La risorsa <bpt id="p1">**</bpt>Registry<ept id="p1">**</ept> in Windows PowerShell DSC (Desired State Configuration) fornisce un meccanismo per gestire i valori e le chiavi del Registro di sistema in un nodo di destinazione.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Syntax</source>
          <target state="translated">Sintassi</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Properties</source>
          <target state="translated">Proprietà</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Property</source>
          <target state="translated">Proprietà</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Description</source>
          <target state="translated">Descrizione</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Key</source>
          <target state="translated">Key</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Indicates the path of the registry key for which you want to ensure a specific state.</source>
          <target state="translated">Indica il percorso della chiave del Registro di sistema per cui si vuole specificare un determinato stato.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>This path must include the hive.</source>
          <target state="translated">Questo percorso deve includere l'hive.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>ValueName</source>
          <target state="translated">ValueName</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Indicates the name of the registry value.</source>
          <target state="translated">Indica il nome del valore del Registro di sistema.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Ensure</source>
          <target state="translated">Ensure</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Indicates if the key and value exist.</source>
          <target state="translated">Indica se la chiave e il valore esistono.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>To ensure that they do, set this property to "Present".</source>
          <target state="translated">Impostare questa proprietà su "Present" per specificare che la chiave e il valore esistono.</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>To ensure that they do not exist, set the property to "Absent".</source>
          <target state="translated">Impostare questa proprietà su "Absent" per specificare che la chiave e il valore non esistono.</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>The default value is "Present".</source>
          <target state="translated">Il valore predefinito è "Present".</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>Force</source>
          <target state="translated">Force</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>If the specified registry key is present, <bpt id="p1">__</bpt>Force<ept id="p1">__</ept> overwrites it with the new value.</source>
          <target state="translated">Se la chiave del Registro di sistema è presente, <bpt id="p1">__</bpt>Force<ept id="p1">__</ept> la sovrascrive con il nuovo valore.</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Hex</source>
          <target state="translated">Hex</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>Indicates if data will be expressed in hexadecimal format.</source>
          <target state="translated">Indica se i dati verranno espressi in formato esadecimale.</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>If specified, the DWORD/QWORD value data is presented in hexadecimal format.</source>
          <target state="translated">Se questa proprietà è specificata, i dati con valori DWORD o QWORD vengono presentati in formato esadecimale.</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>Not valid for other types.</source>
          <target state="translated">La proprietà non è valida per altri tipi.</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>The default value is <bpt id="p1">__</bpt>$false<ept id="p1">__</ept>.</source>
          <target state="translated">Il valore predefinito è <bpt id="p1">__</bpt>$false<ept id="p1">__</ept>.</target>       </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>DependsOn</source>
          <target state="translated">DependsOn</target>       </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>Indicates that the configuration of another resource must run before this resource is configured.</source>
          <target state="translated">Indica che prima di configurare la risorsa è necessario eseguire la configurazione di un'altra risorsa.</target>       </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>For example, if the ID of the resource configuration script block that you want to run first is <bpt id="p1">__</bpt>ResourceName<ept id="p1">__</ept> and its type is <bpt id="p2">__</bpt>ResourceType<ept id="p2">__</ept>, the syntax for using this property is <ph id="ph1">`DependsOn = "[ResourceType]ResourceName"`</ph>.</source>
          <target state="translated">Ad esempio, se l'ID del blocco script di configurazione della risorsa che si vuole eseguire per primo è <bpt id="p1">__</bpt>ResourceName<ept id="p1">__</ept> e il tipo è <bpt id="p2">__</bpt>ResourceType<ept id="p2">__</ept>, la sintassi per usare questa proprietà è <ph id="ph1">`DependsOn = "[ResourceType]ResourceName"`</ph>.</target>       </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>ValueData</source>
          <target state="translated">ValueData</target>       </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>The data for the registry value.</source>
          <target state="translated">Dati per il valore del Registro di sistema.</target>       </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>ValueType</source>
          <target state="translated">ValueType</target>       </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>Indicates the type of the value.</source>
          <target state="translated">Indica il tipo di valore.</target>       </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>The supported types are:</source>
          <target state="translated">I tipi supportati sono:</target>       </trans-unit>
        <trans-unit id="135" translate="yes" xml:space="preserve">
          <source>String (REG_SZ)</source>
          <target state="translated">Stringa (REG_SZ)</target>       </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>Binary (REG-BINARY)</source>
          <target state="translated">Binario (REG-BINARY)</target>       </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>Dword 32-bit (REG_DWORD)</source>
          <target state="translated">Dword a 32 bit (REG_DWORD)</target>       </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>Qword 64-bit (REG_QWORD)</source>
          <target state="translated">Qword a 64 bit (REG_QWORD)</target>       </trans-unit>
        <trans-unit id="139" translate="yes" xml:space="preserve">
          <source>Multi-string (REG_MULTI_SZ)</source>
          <target state="translated">Multistringa (REG_MULTI_SZ)</target>       </trans-unit>
        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>Expandable string (REG_EXPAND_SZ)</source>
          <target state="translated">Stringa espandibile (REG_EXPAND_SZ)</target>       </trans-unit>
        <trans-unit id="141" translate="yes" xml:space="preserve">
          <source>Example</source>
          <target state="translated">Esempio</target>       </trans-unit>
      </group>
