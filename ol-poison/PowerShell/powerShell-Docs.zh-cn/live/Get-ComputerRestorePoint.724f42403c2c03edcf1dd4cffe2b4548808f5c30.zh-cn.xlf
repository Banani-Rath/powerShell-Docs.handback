          <source>Disable-ComputerRestore</source>
          <target state="translated">Disable-computerrestore</target>       </trans-unit>
        <trans-unit id="150" translate="yes" xml:space="preserve">
          <source>Enable-ComputerRestore</source>
          <target state="translated">Enable-computerrestore</target>       </trans-unit>
        <trans-unit id="151" translate="yes" xml:space="preserve">
          <source>Restart-Computer</source>
          <target state="translated">Restart-Computer</target>       </trans-unit>
        <trans-unit id="152" translate="yes" xml:space="preserve">
          <source>Restore-Computer</source>
          <target state="translated">还原计算机</target>       </trans-unit>
