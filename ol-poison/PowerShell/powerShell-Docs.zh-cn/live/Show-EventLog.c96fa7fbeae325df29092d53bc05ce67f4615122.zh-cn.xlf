          <source>Limit-EventLog</source>
          <target state="translated">Limit-EventLog</target>       </trans-unit>
        <trans-unit id="142" translate="yes" xml:space="preserve">
          <source>New-EventLog</source>
          <target state="translated">New-EventLog</target>       </trans-unit>
        <trans-unit id="143" translate="yes" xml:space="preserve">
          <source>Remove-EventLog</source>
          <target state="translated">Remove-EventLog</target>       </trans-unit>
        <trans-unit id="144" translate="yes" xml:space="preserve">
          <source>Write-EventLog</source>
          <target state="translated">Write-EventLog</target>       </trans-unit>
