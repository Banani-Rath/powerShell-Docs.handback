        <trans-unit id="146" translate="yes" xml:space="preserve">
          <source>You can pipe any object to <bpt id="p1">**</bpt>ConvertTo-XML<ept id="p1">**</ept>.</source>
          <target state="translated">Você pode direcionar qualquer objeto para <bpt id="p1">**</bpt>ConvertTo-XML<ept id="p1">**</ept>.</target>       </trans-unit>
