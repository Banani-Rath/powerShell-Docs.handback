        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>You cannot pipe input to Clear-Host.</source>
          <target state="translated">Não é possível redirecionar a entrada para Clear-Host.</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
