        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">概要</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Customizes the behavior of command line editing in PSReadline.</source>
          <target state="translated">自訂命令列中 PSReadline 所編輯的行為。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">語法</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>OptionsSet</source>
          <target state="translated">OptionsSet</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>ColorSet</source>
          <target state="translated">ColorSet</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">描述</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Set-PSReadlineOption<ept id="p1">**</ept> cmdlet customizes the behavior of the PSReadline module when you are editing the command line.</source>
          <target state="translated"> <bpt id="p1">**</bpt>組 PSReadlineOption<ept id="p1">**</ept> cmdlet 可自訂 PSReadline 模組的行為，當您在編輯命令列。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">範例</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Example 1: Set values for Comment type</source>
          <target state="translated">範例 1︰ 設定註解類型的值</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This command sets tokens of the type Comment to be displayed in PSReadline in green text on a gray background.</source>
          <target state="translated">此命令會設定要在 PSReadline 中顯示灰色背景上的綠色文字的註解類型的權杖。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Example 2: Set bell style</source>
          <target state="translated">範例 2︰ 設定鈴聲樣式</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>This cmdlet instructs PSReadline to respond to errors and other conditions that require user input by emitting an audible beep or sound at 1221 Hz for 60 ms.</source>
          <target state="translated">這個指令程式會指示 PSReadline 回應錯誤和其他需要使用者輸入發出嗶或聲音 1221 hz 60 毫秒的條件。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">參數</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>-AddToHistoryHandler</source>
          <target state="translated">-AddToHistoryHandler</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>Specifies a <bpt id="p1">**</bpt>ScriptBlock<ept id="p1">**</ept> that controls which commands get added to PSReadline history.</source>
          <target state="translated">指定 <bpt id="p1">**</bpt>ScriptBlock<ept id="p1">**</ept> 可控制哪些命令加入至 PSReadline 歷程記錄。</target>       </trans-unit>
