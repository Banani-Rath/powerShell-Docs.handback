      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>powershell, cmdlet</source>
          <target state="translated">powershell 指令程式</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Remove-Variable</source>
          <target state="translated">Remove-Variable</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Remove-Variable</source>
          <target state="translated">Remove-Variable</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">概要</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Deletes a variable and its value.</source>
          <target state="translated">刪除變數和它的值。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">語法</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">描述</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Remove-Variable<ept id="p1">**</ept> cmdlet deletes a variable and its value from the scope in which it is defined, such as the current session.</source>
          <target state="translated"> <bpt id="p1">**</bpt>Remove-variable<ept id="p1">**</ept> cmdlet 會從範圍中定義，例如目前的工作階段刪除變數和值。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>You cannot use this cmdlet to delete variables that are set as constants or those that are owned by the system.</source>
          <target state="translated">您無法使用此 Cmdlet 刪除設定為常數或系統所擁有的變數。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">範例</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Example 1: Remove a variable</source>
          <target state="translated">範例 1︰ 移除變數</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>This command deletes the $Smp variable.</source>
          <target state="translated">此命令會刪除 $Smp 變數。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">參數</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>-Confirm</source>
          <target state="translated">-Confirm</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Prompts you for confirmation before running the cmdlet.Prompts you for confirmation before running the cmdlet.</source>
          <target state="translated">會提示您進行確認，然後再執行此指令程式。會提示您進行確認，然後再執行此指令程式。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>-Exclude</source>
          <target state="translated">-排除</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>Specifies an array of items that this cmdlet omits from the operation.</source>
          <target state="translated">指定此 cmdlet 會從作業省略的項目陣列。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>The value of this parameter qualifies the <bpt id="p1">*</bpt>Name<ept id="p1">*</ept> parameter.</source>
          <target state="translated">此參數的值會限定 <bpt id="p1">*</bpt>名稱<ept id="p1">*</ept> 參數。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Enter a name element or pattern, such as "s*".</source>
          <target state="translated">輸入名稱元素或模式，例如 "s*"。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>Wildcards are permitted.</source>
          <target state="translated">允許使用萬用字元。</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>-Force</source>
          <target state="translated">-Force</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Indicates that the cmdlet removes a variable even if it is read-only.</source>
          <target state="translated">指出此 cmdlet 會移除變數，即使它是唯讀。</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>Even using the <bpt id="p1">*</bpt>Force<ept id="p1">*</ept> parameter, the cmdlet cannot remove a constant.</source>
          <target state="translated">即使使用 <bpt id="p1">*</bpt>Force<ept id="p1">*</ept> 參數，cmdlet 仍然無法移除常數。</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>-Include</source>
          <target state="translated">-Include</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>Specifies an array of items that this cmdlet deletes in the operation.</source>
          <target state="translated">在作業中指定此 cmdlet 會刪除的項目陣列。</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>The value of this parameter qualifies the <bpt id="p1">*</bpt>Name<ept id="p1">*</ept> parameter.</source>
          <target state="translated">此參數的值會限定 <bpt id="p1">*</bpt>名稱<ept id="p1">*</ept> 參數。</target>       </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>Enter a name element or pattern, such as s*.</source>
          <target state="translated">輸入名稱元素或模式，例如 s *。</target>       </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>Wildcards are permitted.</source>
          <target state="translated">允許使用萬用字元。</target>       </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>-Name</source>
          <target state="translated">-Name</target>       </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>Specifies the name of the variable to be removed.</source>
          <target state="translated">指定要移除之變數的名稱。</target>       </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>The parameter name (<bpt id="p1">*</bpt>Name<ept id="p1">*</ept>) is optional.</source>
          <target state="translated">參數名稱 (<bpt id="p1">*</bpt>名稱<ept id="p1">*</ept>) 是選擇性的。</target>       </trans-unit>
        <trans-unit id="132" translate="yes" xml:space="preserve">
          <source>-Scope</source>
          <target state="translated">範圍</target>       </trans-unit>
        <trans-unit id="133" translate="yes" xml:space="preserve">
          <source>Gets only the variables in the specified scope.</source>
          <target state="translated">只取得指定範圍內的變數。</target>       </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>The acceptable values for this parameter are:</source>
          <target state="translated">此參數接受的值包括：</target>       </trans-unit>
        <trans-unit id="135" translate="yes" xml:space="preserve">
          <source>Global</source>
          <target state="translated">全域</target>       </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>Local</source>
          <target state="translated">本機</target>       </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>Script</source>
          <target state="translated">指令碼</target>       </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>A number relative to the current scope (0 through the number of scopes, where 0 is the current scope and 1 is its parent)</source>
          <target state="translated">相對於目前範圍 (0 至範圍數目，0 為目前的範圍，1 為其父系) 的數目</target>       </trans-unit>
        <trans-unit id="139" translate="yes" xml:space="preserve">
          <source>Local is the default.</source>
          <target state="translated">預設值為本機。</target>       </trans-unit>
        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>For more information, see about_Scopes.</source>
          <target state="translated">如需詳細資訊，請參閱 about_Scopes。</target>       </trans-unit>
