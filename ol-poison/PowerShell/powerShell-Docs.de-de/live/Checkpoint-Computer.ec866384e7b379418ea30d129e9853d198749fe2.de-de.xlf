        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>SYNOPSIS</source>
          <target state="translated">ZUSAMMENFASSUNG</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Creates a system restore point on the local computer.</source>
          <target state="translated">Erstellt einen Systemwiederherstellungspunkt auf dem lokalen Computer.</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>SYNTAX</source>
          <target state="translated">SYNTAX</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>DESCRIPTION</source>
          <target state="translated">BESCHREIBUNG</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cmdlet creates a system restore point on the local computer.</source>
          <target state="translated">Die <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> -Cmdlet erstellt einen Systemwiederherstellungspunkt auf dem lokalen Computer.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>System restore points and the <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cmdlet are supported only on client operating systems, such as Windows 8, Windows 7, Windows Vista, and Windows XP.</source>
          <target state="translated">Systemwiederherstellungspunkte und das <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> Cmdlet werden nur unter Clientbetriebssystemen wie Windows 8, Windows 7, Windows Vista und Windows XP unterstützt.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Beginning in Windows 8, <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> cannot create more than one checkpoint each day.</source>
          <target state="translated">Ab Windows 8 <bpt id="p1">**</bpt>Checkpoint-Computer<ept id="p1">**</ept> kann nicht mehr als ein Prüfpunkt pro Tag erstellt.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>EXAMPLES</source>
          <target state="translated">BEISPIELE</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>-------------------------- EXAMPLE 1 --------------------------</source>
          <target state="translated">-------------------------- BEISPIEL 1 --------------------------</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>This command creates a system restore point called "Install MyApp".</source>
          <target state="translated">Dieser Befehl erstellt einen Systemwiederherstellungspunkt mit dem Namen %%amp;quot;Install MyApp%%amp;quot;.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>It uses the default APPLICATION_INSTALL restore point type.</source>
          <target state="translated">Er verwendet den Standardtyp für Wiederherstellungspunkte APPLICATION_INSTALL.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>-------------------------- EXAMPLE 2 --------------------------</source>
          <target state="translated">-------------------------- EXAMPLE 2 --------------------------</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>This command creates a MODIFY_SETTINGS system restore point called "ChangeNetSettings".</source>
          <target state="translated">Dieser Befehl erstellt den MODIFY_SETTINGS-Systemwiederherstellungspunkt %%amp;quot;ChangeNetSettings%%amp;quot;.</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>PARAMETERS</source>
          <target state="translated">PARAMETER</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>-Description</source>
          <target state="translated">-Description</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Specifies a descriptive name for the restore point.</source>
          <target state="translated">Gibt einen beschreibenden Namen für den Wiederherstellungspunkt an.</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>This parameter is required.</source>
          <target state="translated">Dieser Parameter ist erforderlich.</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source>-RestorePointType</source>
          <target state="translated">-RestorePointType</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Specifies the type of restore point.</source>
          <target state="translated">Gibt den Typ des Wiederherstellungspunkts an.</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>The default is APPLICATION_INSTALL.</source>
          <target state="translated">Der Standard lautet APPLICATION_INSTALL.</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>Valid values are APPLICATION_INSTALL, APPLICATION_UNINSTALL, DEVICE_DRIVER_INSTALL, MODIFY_SETTINGS, and CANCELLED_OPERATION.</source>
          <target state="translated">Gültige Werte sind APPLICATION_INSTALL, APPLICATION_UNINSTALL, DEVICE_DRIVER_INSTALL, MODIFY_SETTINGS und CANCELLED_OPERATION.</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>CommonParameters</source>
          <target state="translated">CommonParameters</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>This cmdlet supports the common parameters: -Debug, -ErrorAction, -ErrorVariable, -InformationAction, -InformationVariable, -OutVariable, -OutBuffer, -PipelineVariable, -Verbose, -WarningAction, and -WarningVariable.</source>
          <target state="translated">Dieses Cmdlet unterstützt die allgemeinen Parameter:-Debug, - ErrorAction, - ErrorVariable, - InformationAction, -InformationVariable, - OutVariable,-OutBuffer, - PipelineVariable, - Verbose, - WarningAction und -WarningVariable.</target>       </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>For more information, see about_CommonParameters (http://go.microsoft.com/fwlink/?LinkID=113216).</source>
          <target state="translated">Weitere Informationen finden Sie unter "about_commonparameters" (http://go.microsoft.com/fwlink/?LinkID=113216).</target>       </trans-unit>
